.demo {
  padding: 20rpx 0;
}
.card {
  width: 694rpx;
  margin: 16rpx auto;
  padding: 32rpx 28rpx;
  background: #ffffff;
  box-shadow: 0 10px 20px 0 rgba(30, 40, 60, 0.06);
  border-radius: 16px;
  border-radius: 16px;
  box-sizing: border-box;
}
.header {
  padding-bottom: 36rpx;
  border-bottom: 1px dashed #d9d9d9;
  display: flex;
  justify-content: space-between;
}
.header .title {
  font-size: 32rpx;
  color: #1e283c;
}
.header .searchBtn {
  color: #207dfe;
}
.tips {
  padding-top: 45rpx;
  padding-bottom: 34rpx;
  text-align: center;
}
.tips .msg {
  display: block;
  height: 30rpx;
  line-height: 30rpx;
  margin-top: 26rpx;
  font-size: 28rpx;
  color: #b9bec3;
}
.list {
  padding-top: 35rpx;
}
.list .item {
  display: flex;
  height: 50rpx;
  line-height: 50rpx;
  margin-bottom: 37rpx;
}
.list .item:last-child {
  margin-bottom: 0;
}
.list .item .name {
  flex: 1;
  margin-right: 16rpx;
  font-size: 30rpx;
  color: #6e7382;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.list .item .status {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  color: #b9bec3;
}
.list .item text {
  margin-right: 18rpx;
  position: relative;
  bottom: 2rpx;
}

.list .weui-icon-unChecked,
.list .weui-icon-checked {
  width: 40rpx;
  height: 40rpx;
}
